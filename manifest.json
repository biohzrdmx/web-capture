{
	"manifest_version": 2,
	"name": "Web Capture",
	"description": "Capture the current page",
	"version": "1.0",
	"author": "biohzrdmx",
	"permissions": [
		"tabs",
		"http://*/",
		"https://*/"
	],
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"browser_action": {
		"default_icon": "icon16.png"
	},
	"background": {
		"scripts": ["js/extension.js"]
	}
}